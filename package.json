{
  "name": "check-your-css",
  "displayName": "Check Your CSS",
  "description": "Checking compatibility of CSS",
  "publisher": "teamTitans",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other",
    "Linters"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/TeamTitans1/checkyourcss-vscode"
  },
  "license": "MIT",
  "activationEvents": [
    "onCommand:cyc.setSettingJson",
    "onCommand:cyc.fixyourcss"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cyc.setSettingJson",
        "title": "Check Your CSS: Set Browsers and Versions"
      },
      {
        "command": "cyc.fixyourcss",
        "title": "Fix Your CSS"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "Check Your CSS",
      "properties": {
        "checkYourCSS.browsersAndVersions": {
          "type": "array",
          "default": [],
          "description": "Configure browsers and their versions",
          "items": {
            "type": "object",
            "properties": {
              "browserName": {
                "type": "string",
                "description": "browserName"
              },
              "browser": {
                "type": "string",
                "description": "stat"
              },
              "version": {
                "type": "string",
                "description": "version"
              }
            },
            "required": [
              "browser",
              "version"
            ]
          }
        }
      }
    }
  },
  "scripts": {
    "lint": "eslint .",
    "test": "mocha --recursive",
    "test-vscode": "vscode-test"
  },
  "devDependencies": {
    "@babel/parser": "^7.23.9",
    "@babel/traverse": "^7.23.9",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "autoprefixer": "^10.4.17",
    "axios-mock-adapter": "^1.22.0",
    "chai": "^5.1.0",
    "eslint": "^8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "mocha": "^10.3.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "sinon": "^17.0.1"
  },
  "dependencies": {
    "@mdn/browser-compat-data": "^5.5.11",
    "axios": "^1.6.7",
    "typescript": "^5.3.3"
  }
}
